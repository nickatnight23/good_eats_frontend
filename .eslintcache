ge} name = \"description\"/>\n                     <b><p></p></b>\n                   <br/>\n                   <button class=\"ui red button\">Add Restaurant</button>\n\n                    </form> \n                  \n              \n            </div>\n        )\n    }\n}\n\nexport default connect(null,{addRestaurant}) (RestaurantsForm);\n// null means not subscribed to the stores updates\n",["68","69"],"/home/tricia/good_eats_frontend/src/components/Home.js",[],["70","71"],"/home/tricia/good_eats_frontend/src/reducers/usersReducer.js",[],"/home/tricia/good_eats_frontend/src/reducers/rootReducer.js",[],"/home/tricia/good_eats_frontend/src/reducers/restaurantsReducer.js",["72"],"export const restaurantsReducer = (state = {restaurants: [], loading: false}, action) => {\n    switch(action.type) {\n        case(\"LOADING_RESTAURANTS\"):\n        return {...state, loading: true}\n        case ('FETCH_RESTAURANTS'):\n            return {...state, loading: false, restaurants: action.payload}\n        case(\"ADDING_RESTAURANT\"):\n            return {...state, loading: true}\n        case 'ADD_RESTAURANTS':\n                return{...state, loading: false,restaurants: [...state.restaurants, action.payload]}\n        case ('REMOVING_RESTAURANT'):\n                return {...state, loading: true}\n         case ('REMOVE_RESTAURANT'):\n            //   debugger \n            return {...state, loading:  false,\n               restaurants: state.restaurants.filter(restaurant => restaurant.id != action.payload) \n    \n            }\n        default:  \n            return state   \n    }\n}\n\n//the root reducer function is called with the current state and the dispatched action","/home/tricia/good_eats_frontend/src/components/About.js",[],"/home/tricia/good_eats_frontend/src/components/Footer.js",[],["73","74"],"/home/tricia/good_eats_frontend/src/components/Contact.js",[],["75","76"],"/home/tricia/good_eats_frontend/src/index.js",[],"/home/tricia/good_eats_frontend/src/comp